{
  "name": "remote-data-rtk",
  "version": "0.0.2",
  "description": "ADT for @reduxjs/toolkit/query",
  "author": "wintpann",
  "keywords": [
    "ADT",
    "FP",
    "@reduxjs/toolkit/query",
    "functional",
    "fp-ts"
  ],
  "private": false,
  "homepage": "https://github.com/wintpann/remote-data-rtk",
  "repository": {
    "type": "git",
    "url": "https://github.com/wintpann/remote-data-rtk.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wintpann/remote-data-rtk/issues",
    "email": "vvintpann@gmail.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier --write src --ignore-unknown",
    "eslint": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "lint": "yarn typecheck && yarn eslint && yarn prettier",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "build:prod": "npx cross-env NODE_ENV=production rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "npx cross-env DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "build": "yarn test && yarn lint && rimraf dist && yarn build:types && yarn build:prod",
    "release": "yarn build && npx np"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@reduxjs/toolkit": ">=1.0.3",
    "fp-ts/Option": ">=2.0.0",
    "fp-ts/Either": ">=2.0.0",
    "fp-ts/function": ">=2.0.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@rollup/plugin-url": "^7.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "fp-ts": "^2.12.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.1",
    "np": "^7.6.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "storybox-react": "^1.0.13",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .js,.jsx,.ts,.tsx",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {}
}
